{
  "name": "react-put",
  "version": "0.0.3",
  "description": "A flexible formatter for React. This library provides an easy to use i18n interface for React.",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "prepublish": "npm run test; npm run build",
    "build": "./node_modules/.bin/babel es/index.js --out-file index.js",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register tests/index.js",
    "test": "./node_modules/.bin/mocha --require babel-register tests/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericls/react-put.git"
  },
  "keywords": [
    "react",
    "i18n",
    "i10n",
    "format"
  ],
  "author": "Shen Li <dustet@gmail.com> (https://shenli.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericls/react-put/issues"
  },
  "homepage": "https://github.com/ericls/react-put#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "jsdom": "9.9.1",
    "jsdom-global": "2.1.1",
    "mocha": "^3.2.0",
    "react": "^0.14.0 || ^15.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^0.14.0 || ^15.0.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  }
}
